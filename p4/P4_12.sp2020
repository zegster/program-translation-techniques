declare x := 1;
declare y := 2;
{
	in x;
	loop [x > 0] {
		y := x;
		loop [y > 0]
		{
			out y;
			y := y -2;
		};
		x := x- 1;
	};
	out y;
}
#Print input and input-2 down to 1, then input-1 and input-3 down to 1, ...#
